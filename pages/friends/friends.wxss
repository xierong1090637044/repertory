/* pages/mine/friends/friends.wxss */
.item-oper{
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    box-sizing: border-box;
    border-bottom: 1px solid #f4f4f4;
    display: flex;
    justify-content: flex-end;
}
/*自定义按钮*/
.i-swipeout-item view{padding: 0 0 0 5rpx}
.i-swipeout-button-group{
    height:100%;
    width:100%;
}
.i-swipeout-button{
    width:103px;
    float:left;
    display: flex;
    height:100%;
    justify-content: center;
    color:#fff;
    align-items:center;
}
.wrap-2
{
  height:calc(100vh - 184px);
  overflow:scroll;
}