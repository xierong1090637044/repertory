/* pages/mine/mine.wxss */
.content {
	padding-bottom: 20px;
}
.content-box {
	background: #5DAFFC;
	width: 100%;
	height: 200px;
	position: relative;
}
.content-info {
	background-image: url('data:image/png;base64,');
	background-size: 100%;
	height: 200px;
	width: 100%;
	background-position: bottom;
	background-repeat: no-repeat;
	position: absolute;
	left: 0;
	bottom: 0;
}
.content-list {
	width: 100%;
	padding-top: 25px;
	position: relative;
}
.content-item {
	width: 92%;
	height: 80px;
	background: #fff;
	border-radius: 10px;
	padding: 20px 0;
	margin: 0 4%;
	z-index: 2;
	position: relative;
	margin-top: 30px;
}
.content-item-head {
	position: absolute;
	top: -20px;
	left: 40%;
}
.content-item-img {
	width: 60px;
	height: 60px;
	float: left;
}
.content-item-title {
	float: left;
	height: 80px;
	line-height: 80px;
	font-size: 16px;
	font-weight: 700;
	color: #222;
	padding-left: 10px;
}
.content-item-img image {
	width: 100%;
	height: 100%;
	display: block;
	border: none;
	border-radius: 50%;
	box-shadow: 2px 2px 4px #bdbdbd
}
.content-item-text {
	padding-top: 30px;
	text-align:center;
}
.content-item-text .username {
	color:#757575!important;
	display: inline-block;
	text-align: center;
}
.content-item-text text {
	display: inline-block;
	text-align: center;
}
.content-card {
  display: flex;
  align-items: center;
  justify-content: space-between;
	width: 92%;
	height: 35px;
  background-color: #000;
	background-size: cover;
	border-radius: 10px;
	margin: 0 4%;
	z-index: 1;
	position: absolute;
	bottom: -35px;
	padding-top: 62px;
}
.content-card text {
	font-size: 14px;
  padding-left:10px; 
	display: inline-block;
	float: left;
	background-image: -webkit-gradient(linear, 0 0, 0 bottom, from(rgba(170,146,58, 1)), to(rgba(249, 244, 232, 1)));
	-webkit-background-clip: text;
	-webkit-text-fill-color: transparent;
}
.qrcode{
  width: 25px;
  height: 25px;
  float: right;
  margin: 0 30rpx 0 0;
}