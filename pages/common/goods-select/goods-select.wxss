.wrap_overflow
{
  height:calc(100vh - 142px);
  overflow-y:scroll !important;
  background-color: #fff;
  margin-bottom: 10px;
}

.display_flex
{
  display: flex;
  justify-content: space-between;
  align-items: center;
  background: #fff;
  padding: 10px 0 10px 10px;
  border-bottom: 1px solid#ddd;
}
.btn-group
{
  width: 120px;
}

.section
{
  font-size: 26rpx;
  width:33.33%;
  text-align:center;
  background-color:#f6f6f6;
  background:#fff;
}

.picker
{
  width: 90%;
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
}