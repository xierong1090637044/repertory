.page
{
  height: 100vh;
  overflow: scroll;
}
.item
{
  display: flex;
  justify-content: space-between;
  background-color: #fff;
  padding: 10px;
  margin-bottom: 10px;
}
.icon-order
{
  font-size: 22px;
  color: #118fff;
}
.icon-no
{
  font-size: 22px;
  color: #f30;
}
.text-yes
{
  line-height:20px;
