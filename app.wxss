/*标签*/
.label {
	display: inline-block;
	padding: 0.2em 0.3em;
	font-size: 0.6rem;
	line-height: 1;
	color: #616161;
  background-color: #dddddd;
	text-align: center;
	white-space: nowrap;
	vertical-align: middle;
	border-radius: 0.15em;
	position: relative;
}

.label-primary {
	color: #ffffff;
	background-color: #00bcd4;
}
.label-success {
	color: #ffffff;
	background-color: #009688;
}
.label-info {
  color: #ffffff;
  background-color: #03a9f4;
}
.label-warning {
	color: #ffffff;
	background-color: #ffc107;
}
.label-danger {
	color: #ffffff;
	background-color: #e51c23;
}
.label-outlined {
	background-color: transparent;
	position: relative;
}
.label-outlined:after {
  -webkit-border-radius: 2px;
  border-radius: 2px;
  height: 200%;
  content: '';
  width: 200%;
	border: 1px solid #d9d9d9;
  position: absolute;
  top: -1px;
  left: -1px;
  transform: scale(0.5);
  -webkit-transform: scale(0.5);
  transform-origin: 0 0;
  -webkit-transform-origin: 0 0;
  z-index: 1;
}
.label-outlined.label-primary,
.label-outlined.label-primary:after {
	color: #00bcd4;
	border-color: #00bcd4;
}
.label-outlined.label-success,
.label-outlined.label-success:after {
	color: #009688;
	border-color: #009688;
}
.label-outlined.label-info,
.label-outlined.label-info:after {
  	color: #03a9f4;
  	border-color: #03a9f4;
}
.label-outlined.label-warning,
.label-outlined.label-warning:after {
	color: #ffc107;
	border-color: #ffc107;
}
.label-outlined.label-danger,
.label-outlined.label-danger:after {
	color: #e51c23;
	border-color: #e51c23;
}

